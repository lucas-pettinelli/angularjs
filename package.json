{
  "devDependencies": {
    "angular": "^1.8.3",
    "angular-mocks": "^1.8.3",
    "angular-route": "^1.8.3",
    "eslint": "^9.34.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-angular": "^5.0.0",
    "eslint-plugin-html": "^8.1.3",
    "eslint-plugin-prettier": "^5.5.4",
    "husky": "^9.1.7",
    "jasmine-core": "^5.10.0",
    "karma": "^6.4.4",
    "karma-chrome-launcher": "^3.2.0",
    "karma-jasmine": "^5.1.0",
    "lint-staged": "^16.1.6",
    "live-server": "^1.2.2",
    "prettier": "^3.6.2"
  },
  "scripts": {
    "dev": "live-server src/app --port=5173 --entry-file=index.html",
    "lint": "eslint \"**/*.js\" --ext .js",
    "lint:fix": "eslint \"**/*.js\" --ext .js --fix",
    "format": "prettier --write \"**/*.{js,html,css,json,md}\"",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.{js,html,css,json,md}": [
      "prettier --write",
      "eslint --fix",
      "git add"
    ]
  }
}
